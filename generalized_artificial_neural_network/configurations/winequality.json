{
   "network_configuration":{
      "input_vector_size": 10,
      "optimizer": {
         "algorithm": "ADAM",
         "learning_rate":0.00003275,
         "Beta1 (exponential decay rate for the 1st moment estimates)": 0.500,
         "Beta2 (exponential decay rate for the 2st moment estimates)": 0.999,
         "epsilon": 1e-08,
         "locking": false
      },
      "hidden_activation":1,
      "validation_test_interval":25,
      "output_activation": 4,
      "network_dimensions":[ 200, 60, 175, 6 ],
      "cost_function": 0,
      "weight_range":{
         "upper": 1,
         "lower":-1
      }
   },
   "run_configuration":{
      "in_top_k":true,
      "mini-batch-size":250,
      "epochs": 7500
   },
   "visualisation":{
      "display_interval":25,
      "map_layers":[
         {
            "placement": 2,
            "component": "out"
         },{
            "placement": 0,
            "component": "in"
         }
      ],
      "probe_layers":[ {
         "placement": 2,
         "component": "out",
         "points_of_interest": [
            "hist", "avg"
         ]}, {
         "placement": 1,
         "component": "wgt",
         "points_of_interest": [
            "hist", "avg"
         ]}
      ]
   },
   "dataset":{
      "validation_fraction": 0.2,
      "test_fraction": 0.2,
      "name":"wine quality",
      "normalize": true,
      "number_of_classes": 6,
      "total_case_fraction": 1.0
   }
}